//Fazer um programa que imprima a média aritmética dos números 8,9 e 7. A média dos números 4, 5 e 6. A soma das duas médias. A média das médias.

package MediaDaMedia;

import javax.swing.JOptionPane;

public class Media2 {

	public static void main(String[] args) {
		double media1, media2, soma, mediaMedia;
		media1 = (8+9+7)/3;
		media2 = (4+5+6)/3;
		soma = media1 + media2;
		mediaMedia = soma/2;
		
		JOptionPane.showMessageDialog(null, "Média dos números 8, 9 e 7: " + media1 + "\nMédia dos números 4, 5 e 6: " + media2 +  "\nSoma das médias: " + soma + "\nMédia das médias: " +soma);

	}

}
